#include <stdio.h>

int main() {
    int numbers[] = {5, 12, 3, 7, 25, 10};
    int n = sizeof(numbers) / sizeof(numbers[0]);

    int max = numbers[0];

    for (int i = 1; i < n; i++) {
        if (numbers[i] > max) {
            max = numbers[i];
        }
    }

    printf("The maximum element in the array is: %d\n", max);

    return 0;
}
